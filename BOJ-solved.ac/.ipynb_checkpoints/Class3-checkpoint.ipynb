    "c0,c1=0,0\n",
    "def fibo(n):\n",
    "    global c0,c1\n",
    "    if n==0: c0+=1; return 0\n",
    "    if n==1: c1+=1; return 1\n",
    "    return fibo(n-1)+fibo(n-2)\n",
    "for i in range(int(input())):\n",
    "    fibo((int(input())))\n",
    "    print(c0, c1)\n",
    "    c0,c1=0,0"
